file = {:home/edwin/Dropbox/Papers/COVID_Transport/BOX_Literature/Virus survival in environment/Carvalho et al. (2017) - Evaluation of Phi6 Persistence and Suitability as an Enveloped Virus surrogate.pdf:pdf},
issn = {0013-936X},
journal = {Environmental Science & Technology},
month = {aug},
number = {15},
pages = {8692--8700},
title = {{Evaluation of Phi6 Persistence and Suitability as an Enveloped Virus Surrogate}},
