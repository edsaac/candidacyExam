issn = {00456535},
journal = {Chemosphere},
keywords = {Carbohydrates,Chemical oxygen demand,Lipids,Particles,Proteins,Wastewater characterization},
number = {5},
pages = {691--703},
pmid = {15013674},
title = {{Chemical composition associated with different particle size fractions in municipal, industrial, and agricultural wastewaters}},
volume = {55},
